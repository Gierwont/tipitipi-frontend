{
        "name": "strona-fundacja",
        "private": true,
        "version": "0.0.0",
        "type": "module",
        "scripts": {
                "build:server": "cd ./src/backend && mkdir dist && go build -o ./dist/server",
                "start:server": "./src/backend/dist/server &",
