                "test": "./start_server.bat"
        },
        "dependencies": {
                "react": "^18.3.1",
                "react-dom": "^18.3.1",
                "react-router-dom": "^6.26.0"
        },
        "devDependencies": {
                "@types/react": "^18.3.3",
                "@types/react-dom": "^18.3.0",
                "@vitejs/plugin-react": "^4.3.1",
                "autoprefixer": "^10.4.20",
                "globals": "^15.9.0",
                "postcss": "^8.4.41",
                "tailwindcss": "^3.4.10",
                "typescript": "^5.5.3",
                "vite": "^5.4.0"
        }
}