{
        "name": "strona-fundacja",
        "private": true,
        "version": "0.0.0",
        "type": "module",
        "scripts": {
                "build:server": "cd ./src/backend && mkdir dist && go build -o ./dist/server && chmod +x ./dist/server",
                "start:server": "./src/backend/dist/server &",
                "clean": "rm -rf ./src/backend/dist",
                "dev": "bun clean && bun build:server && bun start:server && ./check_server.sh",
                "build": "tsc -b && vite build"
        },
        "dependencies": {
                "react": "^18.3.1",
                "react-dom": "^18.3.1",
                "react-router-dom": "^6.26.0"
        },
        "devDependencies": {
                "@eslint/js": "^9.8.0",
                "@types/react": "^18.3.3",
                "@types/react-dom": "^18.3.0",
                "@vitejs/plugin-react": "^4.3.1",
                "eslint": "^9.8.0",
                "eslint-plugin-react-hooks": "^5.1.0-rc.0",
                "eslint-plugin-react-refresh": "^0.4.9",
                "globals": "^15.9.0",
                "typescript": "^5.5.3",
                "typescript-eslint": "^8.0.0",
                "vite": "^5.4.0"
        }
}